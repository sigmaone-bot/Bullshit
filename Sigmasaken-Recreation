Misc:CreateSection("Pop-ups")

local autoQTE = false

Misc:CreateToggle({
    Name = "Auto Nosf Minigame",
    CurrentValue = false,
    Flag = "AutoQTE",
    Callback = function(v)
        autoQTE = v
    end
})

local pg = LocalPlayer:WaitForChild("PlayerGui")

-- función que intenta clickear un QTE
local function handleQTE(qte)
    if not autoQTE then return end
    if not qte:IsA("Frame") then return end
    if qte.Name ~= "QTE" then return end

    local button = qte:FindFirstChild("ActiveButton")
    if not button then return end

    task.defer(function()
        if not autoQTE then return end

        -- click seguro
        if button.Activate then
            pcall(function()
                button:Activate()
            end)
        else
            for _, conn in ipairs(getconnections(button.MouseButton1Down)) do
                if conn.Function then
                    pcall(conn.Function)
                end
            end
        end
    end)
end

-- escucha TODOS los TemporaryUI
pg.ChildAdded:Connect(function(child)
    if not autoQTE then return end
    if child.Name ~= "TemporaryUI" then return end

    -- QTE que ya exista
    for _, d in ipairs(child:GetChildren()) do
        handleQTE(d)
    end

    -- QTE que aparezca después
    child.ChildAdded:Connect(function(d)
        handleQTE(d)
    end)
end)

Misc:CreateToggle({Name="Anti 1x1x1x1 Popup",CurrentValue=false,Callback=function(v)

    if _G.Anti1x1Loop then task.cancel(_G.Anti1x1Loop); _G.Anti1x1Loop = nil end

    if v then

        _G.Anti1x1Loop = task.spawn(function()

            while task.wait(0.25) do

                local tempUI = LocalPlayer.PlayerGui:FindFirstChild("TemporaryUI")

                if tempUI and tempUI:FindFirstChild("1x1x1x1Popup") then

                    tempUI["1x1x1x1Popup"]:Destroy()

                end

            end

        end)

    end

end})

Misc:CreateSection("Items")

local Workspace = game:GetService("Workspace")

local droppedTools = {}               -- tool names dropped on death
local autoPickupEnabled = false
local hasDropped = false
local currentCharacter

--// Simple alive checker
local function isAlive(char)
	if not char then return false end
	local hum = char:FindFirstChildOfClass("Humanoid")
	return hum and hum.Health > 0
end

--// Drop all tools from backpack + character
local function dropAllTools(char)
	if not char then return end
	table.clear(droppedTools)

	-- Drop all tools from Backpack
	for _, tool in ipairs(LocalPlayer.Backpack:GetChildren()) do
		if tool:IsA("Tool") then
			table.insert(droppedTools, tool.Name)
			tool.Parent = Workspace
		end
	end

	-- Drop all tools from Character
	for _, tool in ipairs(char:GetChildren()) do
		if tool:IsA("Tool") then
			table.insert(droppedTools, tool.Name)
			tool.Parent = Workspace
		end
	end

	hasDropped = true
end

--// Character Added
LocalPlayer.CharacterAdded:Connect(function(char)
	currentCharacter = char
	hasDropped = false

	char:WaitForChild("Humanoid").Died:Connect(function()
		if autoPickupEnabled then
			dropAllTools(char)
		end
	end)
end)

--// Optimized tool pickup loop
task.spawn(function()
	while task.wait(0.25) do  -- 4× more efficient than running every frame
		if not autoPickupEnabled then continue end
		local char = currentCharacter
		if not isAlive(char) then continue end

		local humanoid = char:FindFirstChildOfClass("Humanoid")
		if not humanoid then continue end

		-- Get Map.Ingame reference once
		local map = Workspace:FindFirstChild("Map")
		local ingame = map and map:FindFirstChild("Ingame")

		--// 1. Pick up tools lying in Map.Ingame
		if ingame then
			for _, obj in ipairs(ingame:GetChildren()) do
				if obj:IsA("Tool") then
					humanoid:EquipTool(obj)
				end
			end
		end

		--// 2. Recover tools dropped on death
		for _, toolName in ipairs(droppedTools) do
			local found = Workspace:FindFirstChild(toolName, true)
			if found and found:IsA("Tool") then
				humanoid:EquipTool(found)
			end
		end
	end
end)

--// UI Toggle
Misc:CreateToggle({
	Name = "Auto Pickup Drop Items",
	CurrentValue = false,
	Flag = "AutoPickupTool",
	Callback = function(state)
		autoPickupEnabled = state
	end
})


Misc:CreateSection("Antis")

-- Anti-Kick optimizado
getgenv().AntiBanEnabled = false
local antiKickHooked = false
local remoteCache = {}

-- Función optimizada para aplicar protección (se llama una sola vez)
local function ApplyProtection()
    if antiKickHooked then return end -- Evitar hooks múltiples
    
    -- 1. Hook Kick / Ban en metatable (una sola vez)
    local mt = getrawmetatable(game)
    if not mt then return end
    
    setreadonly(mt, false)
    local oldNamecall = mt.__namecall
    mt.__namecall = newcclosure(function(self, ...)
        local method = getnamecallmethod()
        if method == "Kick" or method == "kick" or tostring(self):lower():find("ban") then
            return nil -- Bloquear completamente
        end
        return oldNamecall(self, ...)
    end)
    setreadonly(mt, true)
    
    -- 2. Hook remotes sospechosos (con cache para no repetir)
    local suspiciousNames = {"BanRemote", "KickRemote", "LogRemote", "ExploitRemote", "AntiCheat"}
    
    for _, name in ipairs(suspiciousNames) do
        -- Usar cache para no buscar repetidamente
        if not remoteCache[name] then
            local remote = ReplicatedStorage:FindFirstChild(name, true)
            if remote then
                if remote:IsA("RemoteEvent") then
                    local oldFire = remote.FireServer
                    remote.FireServer = newcclosure(function(...)
                        return nil -- Bloquear sin warnings
                    end)
                elseif remote:IsA("RemoteFunction") then
                    local oldInvoke = remote.InvokeServer
                    remote.InvokeServer = newcclosure(function(...)
                        return nil -- Bloquear sin warnings
                    end)
                end
                remoteCache[name] = true -- Marcar como hookeado
            else
                remoteCache[name] = false -- Marcar como no encontrado
            end
        end
    end
    
    antiKickHooked = true
end

-- Monitoreo optimizado (sin loops pesados, usa eventos)
local monitoringTask = nil

local function startMonitoring()
    if monitoringTask then return end
    
    monitoringTask = task.spawn(function()
        while task.wait(10) do -- Check cada 10s en lugar de 5s
            if not AntiBanEnabled then break end
            
            -- Solo verificar si hay nuevos remotes
            for name, hooked in pairs(remoteCache) do
                if not hooked then
                    local remote = ReplicatedStorage:FindFirstChild(name, true)
                    if remote and (remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction")) then
                        ApplyProtection() -- Aplicar hooks
                        break -- Salir después de encontrar uno
                    end
                end
            end
        end
        monitoringTask = nil
    end)
end

Misc:CreateButton({
   Name = "Anti Kick (Optimized)",
   Callback = function()
        AntiBanEnabled = true
        
        -- Aplicar protección inmediata
        ApplyProtection()
        
        -- Iniciar monitoreo (si no está ya corriendo)
        if not monitoringTask then
            startMonitoring()
        end
   end,
})

-- Auto-aplicar si ya estaba habilitado
if AntiBanEnabled then
    task.spawn(function()
        task.wait(1)
        ApplyProtection()
        startMonitoring()
    end)
end

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Allowed = {
    NosFlying = true,
    NosFlySlow = true,
    NosCataclysm = true,
    VoidRushDash = true,
    SpeedStatus = true,
    HitSpeedBonus = true,
    Guest1337Charge = true,
    BeheadAbility = true,
    ENRAGED = true,
    Sprinting = true,
    Sk8Trick = true,
    ManicFixation = true,
    FixingGenerator = true,
    Sk8 = true
}

local AntiSlowEnabled = false
local speedConn, ancestryConn, charConn

local function clearConns()
    if speedConn then speedConn:Disconnect(); speedConn = nil end
    if ancestryConn then ancestryConn:Disconnect(); ancestryConn = nil end
end

local function hookCharacter(char)
    clearConns()
    if not AntiSlowEnabled or not char then return end

    local speedMult = char:FindFirstChild("SpeedMultipliers")
    if not speedMult then return end

    -- limpieza inicial
    for _, obj in ipairs(speedMult:GetChildren()) do
        if not Allowed[obj.Name] then
            obj:Destroy()
        end
    end

    -- nuevos objetos
    speedConn = speedMult.ChildAdded:Connect(function(obj)
        if AntiSlowEnabled and obj.Parent == speedMult and not Allowed[obj.Name] then
            obj:Destroy()
        end
    end)

    -- si el character se borra / cambia de parent
    ancestryConn = char.AncestryChanged:Connect(function(_, parent)
        if not parent then
            clearConns()
        end
    end)
end

Misc:CreateToggle({
    Name = "Anti Stun",
    CurrentValue = false,
    Callback = function(state)
        AntiSlowEnabled = state
        clearConns()

        if state then
            if LocalPlayer.Character then
                hookCharacter(LocalPlayer.Character)
            end
            charConn = LocalPlayer.CharacterAdded:Connect(hookCharacter)
        elseif charConn then
            charConn:Disconnect()
            charConn = nil
        end
    end
})

