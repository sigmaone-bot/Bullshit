        for _, player in ipairs(Players:GetPlayers()) do
            local privacy = player:FindFirstChild("PlayerData") 
                and player.PlayerData:FindFirstChild("Settings") 
                and player.PlayerData.Settings:FindFirstChild("Privacy")
            
